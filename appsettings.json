{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning",
        "Umbraco": "Information"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "SanitizeTinyMce": true,
        "Debug": false,
        "Cors": {
          "AllowCredentials": false,
          "AllowedOrigins": [
            "https://app.movekind.se",
            "http://localhost:3000",
            "http://localhost:44367"
          ],
          "AllowedMethods": [
            "GET",
            "POST",
            "OPTIONS"
          ],
          "AllowedHeaders": [
            "Content-Type",
            "Authorization"
          ],
          "ExposedHeaders": []
        },
        "Id": "10b51d2e-4832-4dbf-a6c2-1bee05301fb4"
      },
      "DeliveryApi": {
        "Enabled": true,
        "PublicAccess": true,
        "RichTextOutputAsJson": true,
        "Media": {
          "Enabled": true
        }
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "Unattended": {
        "InstallUnattended": false,
        "UpgradeUnattended": true
      }
    }
  }
}